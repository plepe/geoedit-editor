{
  "stroke": {
    "name": "stroke",
    "type": "paint",
    "default": "#7f7f7f"
  },
  "stroke-width": {
    "name": "stroke width",
    "type": "length",
    "default": "3"
  },
  "stroke-opacity": {
    "name": "stroke opacity",
    "type": "opacity",
    "default": "1"
  },
  "fill": {
    "name": "fill",
    "type": "paint",
    "default": "#7f7f7f",
    "geometry-types": [ "Polygon", "MultiPolygon", "Point" ]
  },
  "fill-opacity": {
    "name": "fill opacity",
    "type": "opacity",
    "default": "0.3",
    "geometry-types": [ "Polygon", "MultiPolygon", "Point" ]
  },
  "radius": {
    "name": "radius",
    "type": "length",
    "default": "10",
    "geometry-types": [ "Point" ]
  },
  "marker": {
    "name": "marker type",
    "type": "marker-type",
    "default": "none",
    "geometry-types": [ "Point" ]
  },
  "marker-stroke": {
    "name": "marker stroke",
    "type": "paint",
    "default": "#000000",
    "geometry-types": [ "Point" ]
  },
  "marker-stroke-width": {
    "name": "marker stroke width",
    "type": "length",
    "default": "1",
    "geometry-types": [ "Point" ]
  },
  "marker-fill": {
    "name": "marker fill",
    "type": "paint",
    "default": "#f2756a",
    "geometry-types": [ "Point" ]
  },
  "offset": {
    "name": "offset",
    "type": "length",
    "default": "0",
    "plugin": "leaflet-polylineoffset",
    "geometry-types": [ "LineString" ]
  }
}
